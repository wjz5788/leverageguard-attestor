# 部署配置
PRIVATE_KEY=你的私钥（用于部署合约）
BASESCAN_API_KEY=你的BaseScan API密钥（用于验证合约）

# 网络配置
# Base 主网 RPC URL
BASE_RPC_URL=https://mainnet.base.org

# Base Sepolia 测试网 RPC URL  
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# 合约配置
BASE_USDC_ADDRESS=0x833589fCD6EdB6E08f4c7C32D4f71B54Bda02913
TREASURY_ADDRESS=你的金库地址

# Gas 报告配置（可选）
REPORT_GAS=false
COINMARKETCAP_API_KEY=你的CoinMarketCap API密钥

# 部署网络选择
# 可选值: base, baseSepolia, localhost
DEPLOY_NETWORK=baseSepolia